{
    "manifest_version": 2,
    "name": "Cacao CORS Proxy",
    "version": "0.2.0-a1",

    "author": "Michael FIG",

    "description": "Cacao (CORS Access-Control-Allow-Origin) Proxy",
    "short_name": "Cacao",
    "homepage_url": "https://github.com/michaelfig/cacao-extension",
    "icons": {
        "48": "cacao48.png",
        "96": "cacao96.png",
        "128": "cacao128.png"
    },

    "permissions": [
        "*://*/*",
        "webRequest",
        "webRequestBlocking"
    ],

    "background": {
        "scripts": [
            "browser-polyfill.min.js",
            "cacao-bg.js"
        ]
    }
}
